{
	"src": [
		"src/**/*.md"
	],
	"dist": "docs",
	"outputTypes": ["docx", "latex"],
	"templates": {
		"docx": {
			"options": {
			},
			"variables": {
			}
		},
		"latex": {
			"options": {
				"pdf-engine": "xelatex"
			},
			"variables": {
				"documentclass": "report",
				"papersize": "a4",
				"mainfont": "NanumMyeongjo",
				"geometry": "left=3cm,right=3cm,top=2cm,bottom=2cm",
				"linestretch": "1.25"
			}
		}
	}
}